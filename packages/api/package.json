{
    "name": "@impactmarket/api",
    "main": "./dist/app.js",
    "version": "1.0.0",
    "license": "Apache-2.0",
    "engines": {
        "node": "18.x"
    },
    "scripts": {
        "dev": "nodemon --config nodemon.json src/app.ts",
        "start": "node --require './appsignal.cjs' dist/app.js",
        "start:local": "RUN_LOCAL_BUILD=true CLIMEM=8999 node -r climem dist/app.js",
        "test": "NODE_ENV=test mocha",
        "build:prepare": "tsc --noEmit",
        "build": "babel src --out-dir dist --extensions \".js,.ts\" --copy-files --source-maps inline",
        "climem": "climem 8999",
        "test:load": "autocannon -c 100 -d 120 -p 10 http://localhost:5002/api/v2/stories",
        "prettier:dryrun": "prettier -l \"./**/*.ts\"",
        "eslint:dryrun": "eslint \"./**/*.ts\" --max-warnings=0",
        "prettier": "prettier --write \"./**/*.ts\"",
        "eslint": "eslint --fix \"./**/*.ts\" --max-warnings=0"
    },
    "devDependencies": {
        "@babel/cli": "7.23.0",
        "@babel/core": "7.23.2",
        "@babel/preset-env": "7.23.2",
        "@babel/preset-typescript": "7.23.2",
        "@types/cors": "2.8.13",
        "@types/express": "4.17.17",
        "@types/express-rate-limit": "6.0.0",
        "@types/json2csv": "5.0.3",
        "@types/jsonwebtoken": "9.0.2",
        "@types/mocha": "10.0.2",
        "@types/morgan": "1.9.4",
        "@types/multer": "1.4.7",
        "@types/rate-limit-redis": "3.0.0",
        "autocannon": "7.14.0",
        "babel-plugin-module-resolver": "5.0.0",
        "babel-plugin-tsconfig-paths-module-resolver": "1.0.4",
        "climem": "2.0.0",
        "express-status-monitor": "1.3.4",
        "mocha": "10.2.0",
        "nodemon": "3.0.1",
        "prettier": "3.2.4"
    },
    "dependencies": {
        "@appsignal/nodejs": "3.0.14",
        "@celo/base": "5.0.2",
        "@celo/identity": "5.1.0",
        "@impactmarket/core": "1.0.0",
        "@slack/events-api": "3.0.1",
        "bignumber.js": "9.1.1",
        "body-parser": "1.20.2",
        "celebrate": "15.0.1",
        "cors": "2.8.5",
        "csvtojson": "2.0.10",
        "ethers": "5.7.2",
        "express": "4.18.2",
        "express-rate-limit": "7.1.0",
        "helmet": "7.1.0",
        "json2csv": "5.0.7",
        "jsonwebtoken": "9.0.0",
        "module-alias": "2.2.2",
        "morgan": "1.10.0",
        "multer": "1.4.5-lts.1",
        "rate-limit-redis": "4.2.0",
        "swagger-jsdoc": "6.2.8",
        "swagger-ui-express": "5.0.0",
        "ts-node": "10.9.1",
        "tsconfig-paths": "4.2.0",
        "twilio": "4.19.0",
        "typescript": "5.2.2"
    }
}

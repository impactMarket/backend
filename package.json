{
    "name": "@impact-market/api",
    "version": "1.3.1",
    "description": "Decentralized impact-driven 2-sided marketplace to provide financial services to charities and vulnerable beneficiaries.",
    "scripts": {
        "start": "nodemon --config nodemon.json src/api/app.ts",
        "worker-dev": "nodemon -w src/worker src/worker/index.ts",
        "serve": "node dist/api/app.js",
        "worker": "node dist/worker/index.js",
        "build": "tsc -p .",
        "build:watch": "tsc -p . --watch",
        "postinstall": "yarn build",
        "prettier": "prettier --write \"./**/*.ts\"",
        "eslint": "eslint --fix \"./**/*.ts\"",
        "prettier-no-fix": "prettier --write \"./**/*.ts\"",
        "eslint-no-fix": "eslint --fix \"./**/*.ts\"",
        "test-db-migrate": "NODE_ENV=test sequelize db:migrate --env test",
        "test": "NODE_ENV=test mocha",
        "coverage": "NODE_ENV=test nyc --reporter=html mocha",
        "precoverage:ci": "NODE_ENV=test sequelize db:migrate --env ci",
        "coverage:ci": "NODE_ENV=test nyc --reporter=lcov mocha",
        "test:watch": "mocha --watch",
        "clean": "rm -rf dist coverage .nyc_output"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/impactMarket/api.git"
    },
    "author": "impact-market",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/impactMarket/api/issues"
    },
    "homepage": "https://github.com/impactMarket/api#readme",
    "engines": {
        "node": "14.15.1"
    },
    "_moduleAliases": {
        "@controllers": "dist/api/controllers",
        "@validators": "dist/api/validators",
        "@models": "dist/database/models",
        "@services": "dist/services",
        "@utils": "dist/utils",
        "@interfaces": "dist/interfaces"
    },
    "dependencies": {
        "@hubspot/api-client": "5.0.0",
        "@sendgrid/mail": "7.6.0",
        "@sentry/node": "6.14.1",
        "@sentry/tracing": "6.14.1",
        "@slack/events-api": "3.0.1",
        "@slack/web-api": "6.5.0",
        "apicache": "1.6.2",
        "aws-sdk": "2.1024.0",
        "axios": "0.24.0",
        "bignumber.js": "9.0.0",
        "body-parser": "1.19.0",
        "celebrate": "15.0.0",
        "cors": "2.8.5",
        "cron": "1.8.2",
        "dotenv": "10.0.0",
        "envalid": "7.2.2",
        "ethers": "5.5.1",
        "expo-server-sdk": "3.6.0",
        "express": "4.17.1",
        "express-rate-limit": "5.5.1",
        "faker": "5.5.3",
        "helmet": "4.6.0",
        "jsonwebtoken": "8.5.1",
        "lodash": "4.17.21",
        "mathjs": "10.0.0",
        "module-alias": "2.2.2",
        "moment": "2.29.1",
        "morgan": "1.10.0",
        "node-schedule": "2.0.0",
        "pg": "8.7.1",
        "pg-hstore": "2.3.4",
        "pg-native": "3.0.0",
        "rate-limit-redis": "2.1.0",
        "redis": "3.1.2",
        "sequelize": "6.6.0",
        "sequelize-cli": "6.3.0",
        "sha3": "2.1.4",
        "swagger-jsdoc": "6.1.0",
        "swagger-ui-express": "4.1.6",
        "winston": "3.3.3"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "1.0.1",
        "@types/bluebird": "3.5.33",
        "@types/chai": "4.2.18",
        "@types/cors": "2.8.10",
        "@types/cron": "1.7.2",
        "@types/express": "4.17.13",
        "@types/express-rate-limit": "5.1.3",
        "@types/jsonwebtoken": "8.5.0",
        "@types/mocha": "8.2.2",
        "@types/node": "14.14.12",
        "@types/node-schedule": "1.3.1",
        "@types/pg": "8.6.1",
        "@types/rate-limit-redis": "1.7.2",
        "@types/redis": "2.8.32",
        "@types/sinon": "10.0.2",
        "@types/validator": "13.0.0",
        "@typescript-eslint/eslint-plugin": "3.4.0",
        "@typescript-eslint/parser": "3.4.0",
        "chai": "4.3.4",
        "chai-http": "4.3.0",
        "chai-subset": "1.6.0",
        "coveralls": "3.1.0",
        "eslint": "7.3.1",
        "eslint-config-prettier": "6.11.0",
        "eslint-config-universe": "4.0.0",
        "fake": "0.2.2",
        "ganache-cli": "6.12.2",
        "mocha": "9.0.0",
        "nodemon": "2.0.7",
        "nyc": "15.1.0",
        "prettier": "2.3.1",
        "proxyquire": "2.1.3",
        "sequelize-mock": "0.10.2",
        "sequelize-test-helpers": "1.3.3",
        "sinon": "11.1.1",
        "source-map-support": "0.5.19",
        "timekeeper": "2.2.0",
        "ts-node": "9.0.0",
        "typescript": "3.8.3"
    }
}
